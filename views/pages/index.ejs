<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>

  <body class="container">
    <header><%- include('../partials/header'); %></header>

    <main>
      <div class="row">
        <div class="col-sm-8">
          <div class="jumbotron">
            <h1>Coffee is great</h1>
            <p>Welcome to my coffee journals</p>
            <h2>Events</h2>
            <p><%= tagline %></p>
            <ul>
              <% competitors.forEach(function(competitor) { %>
              <li>
                <strong><%= competitor.name %></strong>
                representing <%= competitor.organization %>
              </li>
              <% }); %>
            </ul>
          </div>
        </div>
        <div class="col-sm-4">
          <div class="well">
            <h3>Look I'm A Sidebar!</h3>
          </div>
        </div>
      </div>
    </main>

    <footer><%- include('../partials/footer'); %></footer>
  </body>
</html>
